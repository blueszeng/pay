{
  "restartable": "rs",
  "ignore": [
    ".git",
    "node_modules/**/node_modules"
  ],
  "verbose": true,
  "execMap": {
    "js": "node --harmony"
  },
  "events": {
  },
  "watch": [
    "app.js",
    "app/",
    "routes/",
    "config/",
    "controllers/",
    "middlewares/",
    "models/",
    "services/",
    "utils/"
  ],
  "env": {
    "NODE_ENV": "development"
  },
  "ext": "js json"
}
